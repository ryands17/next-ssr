    "@testing-library/react": "12.1.2",
    "@types/autoprefixer": "10.2.0",
    "@types/jest": "27.4.0",
    "@types/node": "14.18.4",
    "@types/prettier": "2.4.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
